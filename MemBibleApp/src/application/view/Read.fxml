<?xml version="1.0" encoding="UTF-8"?>

<?import com.gluonhq.charm.glisten.control.CharmListView?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.MemBibleController">
   <children>
      <Pane prefHeight="500.0" prefWidth="800.0">
         <children>
            <Button mnemonicParsing="false" onAction="#goHome" prefHeight="31.0" prefWidth="74.0">
               <graphic>
                  <ImageView fx:id="goHome" fitHeight="26.0" fitWidth="22.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../Pics/340.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <TabPane layoutY="32.0" prefHeight="375.0" prefWidth="506.0" tabClosingPolicy="UNAVAILABLE" tabMaxWidth="200.0" tabMinWidth="150.0">
              <tabs>
                <Tab text="Book">
                     <content>
                        <CharmListView />
                     </content>
                  </Tab>
                <Tab text="Chapter" />
                  <Tab text="Verse">
                    <content>
                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                    </content>
                  </Tab>
              </tabs>
            </TabPane>
         </children></Pane>
   </children>
</AnchorPane>
